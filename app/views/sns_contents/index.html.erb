<p id="notice"><%= notice %></p>

<h1>Listing Sns Contents</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sns_contents.each do |sns_content| %>
      <tr>
        <td><%= link_to 'Show', sns_content %></td>
        <td><%= link_to 'Edit', edit_sns_content_path(sns_content) %></td>
        <td><%= link_to 'Destroy', sns_content, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sns content', new_sns_content_path %>
